<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://integration.sprint.com/common/header/WSMessageHeader/v2" targetNamespace="http://integration.sprint.com/common/header/WSMessageHeader/v2" elementFormDefault="qualified" attributeFormDefault="unqualified" version="2.10">
